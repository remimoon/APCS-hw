
public class Shapes {
    public String box(int r, int c) {
	String s = "";

	while (r>0){
	    int cc = 0;
	    while (cc < c){
		s += "*";
		cc += 1;
	    }
	    s = s + "\n";
	    r--;
	}

	return s;
    }
    public String tri1(int h) {
	String s = "";
	while (r>0){
	    s += "*";
	    s = s + "\n";
	    h--;
	}
    }
}
